<% provide :title, t("users.new.sign_up") %>
<h1><%= t "users.new.sign_up" %></h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @user, url: signup_path do |f| %>
      <%= render "shared/error_messages", object: f.object %>
      <%= render "form" %>
    <% end %>
  </div>
</div>
